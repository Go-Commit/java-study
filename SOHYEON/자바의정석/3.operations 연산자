public class TI_java_part1 {
	public static void main(String[] args) {
	
	int val = 356;
	System.out.println((val == 356) ? "300" : false);
	
	}
}


--------------------------
확인 6
--------------------------
public class TI_java_part1 {
	public static void main(String[] args) {
		
		int lengthTop = 5;
		int lengthBottom = 10;
		int height = 7;
		double area = ((lengthTop + lengthBottom) * height);
		System.out.println(area);
		
	}
}


