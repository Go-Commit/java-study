	public static void main(String[] args) {
	
	int score = 85;
	String result = (!(score>90))? "가":"나";
	System.out.println(result);
		
	}


-------------------------
확인4/ 17 24
-------------------------

public class TI_java_part1 {
	public static void main(String[] args) {
	
	int pen = 534;
	int std = 30;
	
	int pen_std = (pen / std);
	System.out.println(pen_std);
	
	int pen_left = (pen % std);
	System.out.println(pen_left);
	
	}
}
